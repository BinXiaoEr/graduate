{"remainingRequest":"/Users/shenchenbin/Desktop/NetCloud_vue/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/shenchenbin/Desktop/NetCloud_vue/src/components/Home.vue?vue&type=style&index=0&id=8dc7cce2&lang=less&scoped=true&","dependencies":[{"path":"/Users/shenchenbin/Desktop/NetCloud_vue/src/components/Home.vue","mtime":1586685541125},{"path":"/Users/shenchenbin/Desktop/NetCloud_vue/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/shenchenbin/Desktop/NetCloud_vue/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/shenchenbin/Desktop/NetCloud_vue/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/shenchenbin/Desktop/NetCloud_vue/node_modules/less-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/shenchenbin/Desktop/NetCloud_vue/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/shenchenbin/Desktop/NetCloud_vue/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Home.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Home.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <el-container class=\"home-container\">\n    <!-- hearer 头部区域 -->\n    <el-header>\n      <div>\n        <img src=\"../assets/heima.png\" alt />\n        <span>电商后台管理系统</span>\n      </div>\n      <el-button type=\"info\" @click=\"logout\">退出</el-button>\n    </el-header>\n    <!-- container 是主页面区域 -->\n    <el-container>\n      <!-- aside 是侧边栏 -->\n      <el-aside width=\"200px\">\n        <!-- 侧边栏组件 -->\n        <el-menu\n          background-color=\"#333744\"\n          text-color=\"#fff\"\n          active-text-color=\"#409eff\"\n          :unique-opened=\"true\"\n          :router=\"true\"\n          :default-active=\"activepath\" \n        >\n        <!-- default-active=\"/30\"  设置当前左测被激活颜色  -->\n          <!-- 1级菜单 -->\n          <el-submenu :index=\"item.id+''\" v-for=\"item in menuslist\" :key=\"item.id\">\n            <!-- 一级菜单模板区域 -->\n            <template slot=\"title\">\n              <!-- 图标 -->\n              <i :class=\"iconobj[item.id]\"></i>\n              <!-- 文本 -->\n              <span>{{item.authName}}</span>\n            </template>\n            <!-- 二级菜单 -->\n            <el-menu-item :index=\"'/'+subitem.id+''\" v-for=\"subitem in item.children\" :key=\"subitem.id\" @click=\"saveState('/'+subitem.id)\">\n              <!-- 二级菜单模板区域 -->\n              <template slot=\"title\">\n                <!-- 图标 -->\n                <i class=\"el-icon-menu\"></i>\n                <!-- 文本 -->\n                <span>{{subitem.authName}}</span>\n              </template>\n            </el-menu-item>\n          </el-submenu>\n        </el-menu>\n      </el-aside>\n      <!-- main 是右边内容 -->\n      <el-main>\n        <!-- 放一个路由占位符 -->\n        <router-view></router-view>\n      </el-main>\n    </el-container>\n  </el-container>\n</template>\n\n<script>\nimport service from \"../service/BaseDao\";\nexport default {\n  // created 进这个界面自动调用\n  data() {\n    return {\n      menuslist: [],\n      iconobj: {\n        \"1\": \"iconfont icon-user\",\n        \"2\": \"iconfont icon-tijikongjian\",\n        \"3\": \"iconfont icon-shangpin\",\n        \"4\": \"iconfont icon-danju\",\n        \"5\": \"iconfont icon-baobiao\"\n      },\n      // 被激活的链接地址\n      activepath:''\n    };\n  },\n\n  created() {\n    this.getMenulist();\n    this.activepath=window.sessionStorage.getItem('activepath')\n  },\n  methods: {\n    logout() {\n      window.sessionStorage.clear(); // 清楚缓存\n      this.$router.push(\"/login\"); // 重定向到登录页面\n    },\n    // 获取所有的菜单\n    getMenulist() {\n      service.post(\"/user/menus/\", {}).then(data => {\n        if (data.state == 1) {\n          this.menuslist = data.data;\n          console.log(this.menuslist);\n        }\n        //console.log(data) data 是后台返回的json数据\n      });\n    },\n    // 保存链接的激活状态\n    saveState(activepath){\n        window.sessionStorage.setItem('activepath',activepath)\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.el-header {\n  background-color: #373d41;\n  display: flex;\n  justify-content: space-between;\n  padding-left: 0;\n  align-items: center;\n  color: #ffff;\n  font-size: 20px;\n  > div {\n    display: flex;\n    align-items: center;\n    span {\n      margin-left: 15px;\n    }\n  }\n}\n\n.el-aside {\n  background-color: #333744;\n}\n.el-main {\n  background-color: #eaedf1;\n}\n.home-container {\n  height: 100%;\n}\n.el-menu {\n  border-right: none;\n}\n.iconfont {\n  padding: 10px;\n}\n</style>\n\n\n"]}]}